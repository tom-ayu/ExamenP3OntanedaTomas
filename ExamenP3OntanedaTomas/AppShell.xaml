<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ExamenP3OntanedaTomas.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ExamenP3OntanedaTomas"
    Shell.FlyoutBehavior="Locked"
    Title="Examen P3 Tomás Ontaneda">

    <ShellContent
        Title="Primera Pantalla"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
    Title="Segunda Pantalla"
    ContentTemplate="{DataTemplate local:SegundaPantalla}"
    Route="SegundaPantalla" />

    <TabBar>
        <Tab
        Title="Primera Página" Route="HomePage"/>
        <Tab
        Title="Segunda Página" Route="SegundaPagina" />
    </TabBar>

</Shell>
